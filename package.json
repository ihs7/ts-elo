    "@types/jest": "^30.0.0",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "jest": "^30.1.3",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.1",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.43.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "jest": {
    "preset": "ts-jest",
    "testPathIgnorePatterns": [
      "dist"
