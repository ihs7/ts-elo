{
  "name": "@ihs7/ts-elo",
  "description": "Elo scoring system implemented in TypeScript",
  "author": "Ívar Haukur Sævarsson",
  "version": "2.0.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc --outDir dist",
    "prettier": "prettier --write './src/**/*.ts'",
